CHATBOT_URL=https://your-chatbot.example.com
HEADLESS=true
SLOW_MO_MS=0
OUTPUT_FILE=outputs/results.jsonl
# Optional: if your chatbot requires login cookies, store a Playwright storage state file path
STORAGE_STATE=./storageState.json

# Auth cookie details (you must set these)
AUTH_COOKIE_NAME=your_auth_cookie_name
AUTH_COOKIE_VALUE=your_cookie_value
AUTH_COOKIE_DOMAIN=your-custom-app.example.com
AUTH_COOKIE_PATH=/

# Optional: if cookie is secure / sameSite
AUTH_COOKIE_SECURE=true
AUTH_COOKIE_SAMESITE=Lax  # Lax|Strict|None


# Selectors for your app:
CHAT_INPUT_SELECTOR=textarea[data-testid="chat-input"]
SEND_BUTTON_SELECTOR=button[data-testid="send"]
ASSISTANT_MESSAGE_SELECTOR=[data-testid="assistant-message"]
THINKING_SELECTOR=[data-testid="thinking"]

# Optional: if the app needs a "new chat" click per prompt batch
NEW_CHAT_SELECTOR=button[data-testid="new-chat"]